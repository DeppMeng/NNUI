{
    "name": "LeNet-5",
    
    "train":{
        "epochs": 20,
        "batch_size": 100,
        "optimizer": "adam",
        "inputs":{
            "data":{
                "dataset": "mnist",
                "datas": ["train_x", "train_y"],
                "shapes": [[28, 28, 1], [1]],
                "batch_size": 100
            }
        },
        "outputs":{
            "pred":{
                "loss": "categorical_crossentropy",
                "loss_weight": 1.0, 
                "metrics": ["accuracy"]
            }
        }
    },
    
    "operators":[
        {
            "name": "data",
            "type": "Data",
            "output": "data"
        },
        {
            "name": "conv1",
            "type": "Conv",
            "dim_out": 20,
            "kernel": 5,
            "input": "data"
        },
        {
            "name": "pool1",
            "type": "Pool",
            "pool": "MAX",
            "kernel": 2,
            "stride": 2
        },
        {
            "name": "conv2",
            "type": "Conv",
            "dim_out": 50,
            "kernel": 5
        },
        {
            "name": "pool2",
            "type": "Pool",
            "pool": "MAX",
            "kernel": 2,
            "stride": 2
        },
        {  
            "name": "fc3",
            "type": "FC",
            "dim_out": 500
        },
        {
            "name": "relu3",
            "type": "ReLU"
        },
        {  
            "name": "fc4",
            "type": "FC",
            "dim_out": 10
        },
        {
            "name": "pred",
            "type": "Softmax"
        }
    ]
}