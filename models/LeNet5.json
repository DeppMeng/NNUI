{
    "operators":[
        {
            "name": "data",
            "type": "Data",
            "batch_size": 100,
            "dataset": "mnist",
            "outputs": ["data", "label"]
        },
        {
            "name": "conv1",
            "type": "Conv",
            "dim_out": 20,
            "kernel": 5,
            "input": "data"
        },
        {
            "name": "pool1",
            "type": "Pool",
            "pool": "MAX",
            "kernel": 2,
            "stride": 2
        },
        {
            "name": "conv2",
            "type": "Conv",
            "dim_out": 50,
            "kernel": 5
        },
        {
            "name": "pool2",
            "type": "Pool",
            "pool": "MAX",
            "kernel": 2,
            "stride": 2
        },
        {  
            "name": "fc3",
            "type": "FC",
            "dim_out": 500
        },
        {
            "name": "relu3",
            "type": "ReLU"
        },
        {  
            "name": "pred",
            "type": "FC",
            "dim_out": 10
        },
        {
            "name": "softmax",
            "type": "Softmax",
            "inputs": ["pred"]
        }
    ]
}